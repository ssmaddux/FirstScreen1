                <TableColumn fx:id="associatedPartIdColumn" prefWidth="75.0" text="C1" />
                <TableColumn fx:id="associatedPartNameColumn" prefWidth="75.0" text="C2" />
                <TableColumn fx:id="associatedPartInventoryColumn" prefWidth="75.0" text="C1" />
                <TableColumn fx:id="associatedPartPriceColumn" prefWidth="75.0" text="C1" />
            </columns>
        </TableView>
        <TableView fx:id="partTableView" layoutX="385.0" layoutY="81.0" prefHeight="135.0" prefWidth="347.0">
            <columns>
                <TableColumn fx:id="partIdColumn" prefWidth="75.0" text="C1" />
                <TableColumn fx:id="partNameColumn" prefWidth="75.0" text="C2" />
                <TableColumn fx:id="partInventoryColumn" prefWidth="75.0" text="C1" />
