        <Label layoutX="33.0" layoutY="174.0" text="Inv" />
        <Label layoutX="33.0" layoutY="207.0" text="Price" />
        <Label layoutX="33.0" layoutY="242.0" text="Max" />
        <Label layoutX="201.0" layoutY="242.0" text="Min" />
